                                                    }
                                                    @if (Model.Latest[j].OriginBn != null)
                                                    {
                                                        <a href='@Url.Action("Details", "LiveAnimal", new {Id = @Model.Latest[j].Id})'>@Model.Latest[j].OriginBn</a>
                                                    }
                                                    else
                                                    {
                                                        <a href='@Url.Action("Details", "LiveAnimal", new {Id = @Model.Latest[j].Id})'>উত্স: নাই</a>
                                                    }
                                                    @if (!double.IsNaN(Model.Latest[j].Price))
                                                    {
                                                        <p class="price">&#2547 @Model.Latest[j].Price</p>
                                                    }
                                                    else
                                                    {
                                                        <p class="price">&#2547 নাই</p>
                                                    }
                                                    <script>
                                    var num = @Model.Latest[j].Price;
                                    $("#@Model.Latest[j].Id")
                                        .text("৳ " +replaceNumbers(num.toString()));
                                    console.log(replaceNumbers(num.toString()));

                                                    </script>
                                                </div>
                                            </li>
                                        }
                                    </ul>
                                </div>
                            }
                        </div>
